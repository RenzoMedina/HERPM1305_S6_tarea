<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/my_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/my_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Renzo_medina_seccioncurso"
        tools:targetApi="31">
        <activity
            android:name=".UpdateInfractionActivity"
            android:label="Update Infraction Activity"
            android:parentActivityName=".ListInfractionActivity"
            android:exported="true" />
        <activity
            android:name=".ListInfractionActivity"
            android:exported="true"
            android:label="List Infraction Activity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".InfractionActivity"
            android:exported="true"
            android:label="Infraction Activity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>